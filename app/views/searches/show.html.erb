<h1>Search Results</h1>
<p><%= link_to "Back", new_search_path %></p>

<% if @search.search_cars.empty? %>
    <p>No Pokemon were found.</p>
<% else %>
<% @search.search_cars.each do |car| %>

<%= link_to car_path(car) do %>
<%= car.model.manufacturer.name %>
<%= car.model.name %>
<%= car.price %>
<%= car.manufacture_year %>
<%= car.mileage %>
<% end %>
<% end %>

<% end %>